		$("#es_top_button").on("click touch", function(){
			$('body, html').stop().animate({scrollTop:0}, 200);
			
			try {
				GA_Event('TAB','TOP','TOP');
			} catch (e) {}
		});
		
		var btn_common_top = $("#btn_common_top");
		var fix_scrollWeb = $(".fix_scrollWeb");
		var fix_Bottom = $(".fix_Bottom");

		var $winH = $(window).height();
		var $cont = $('#container');
		var $footer = $('#footer');
		var headerH = $('#m_header').height();
		var $btnQ = $('.qmenu_m .btn_q');
		var $qmenuW = $('#qmenu_w');
		var $qmenuS = $('#qmenu_side');
		var $qmenuM = $('#qmenu_m');
		var $tb_qmenu = $('#qmenu_w.loc_tb');
		var isScroll = false;
		var didScroll = false;
		var lastST = 0;
		var viewArea = $winH / 3;

		var qmenuTop;
		if ( $('#shop_topLayer').length > 0 ) {
			qmenuTop = parseInt($('#container').css('padding-top')) + $('#shop_topLayer').innerHeight();
			$qmenuW.addClass('loc_tb');
			$qmenuM.hide();
		}
		if ( $('.mixArea').length > 0 ) {
			$qmenuW.hide();
			$qmenuM.hide();
		}

		var qmenuTopPosition = {
			'top' : qmenuTop + 'px',
		}
		var qmenuScrolledPosition = {
			'top' : '50%',
		}
		var qmenuBottomPosition = {
			'top' : 'auto',
		}

		var convPoint;
		var stopPoint;
		var srl_wh = $(window).height();
	
		if ( $(window).scrollTop() < viewArea) {
			if ( $('#shop_topLayer').length > 0 ) {
				$qmenuW.addClass('top');
				$qmenuS.css(qmenuTopPosition);
			}
		}

		$(window).scroll(function() {
			
			var srlTop  = $(this).scrollTop();
			var btn_offset = btn_common_top.offset().top;
			var srl_wh = $(window).height();

			var convPoint = (srl_wh / 2) + ($qmenuS.outerHeight() / 2);
			var stopPoint = $footer.offset().top - (srl_wh / 2) - ($qmenuS.outerHeight() / 2);

			if( srlTop > 800 ){
				btn_common_top.find("ul").css("display","block");
				
				if ($winW > 1024) {
					if ( fix_scrollWeb.length > 0 && fix_scrollWeb.css('bottom') == '0px') {
						btn_common_top.find("ul").addClass("fixbar")
					} else{
						btn_common_top.find("ul").removeClass("fixbar")
					}
				} else{
					if (fix_Bottom.length > 0) {
						btn_common_top.find("ul").addClass("fixbar")
					}
				}
			}
			else {
				btn_common_top.find("ul").hide();
			}
			
			if ((srlTop + srl_wh) >= btn_offset) {
				btn_common_top.addClass("stop");
			} else{
				btn_common_top.removeClass("stop");
			}

			if (convPoint > stopPoint) {
				if ( srlTop >= stopPoint ) {
					if ( !$qmenuW.hasClass('bottom')) {
						$qmenuS.css(qmenuBottomPosition);
						$qmenuW.addClass('bottom');
					}
				} else {
					if ( $qmenuW.hasClass('bottom') ){
						$qmenuW.removeClass('bottom');
					}
					$qmenuS.css(qmenuScrolledPosition);
				}
			} else {
				if (srlTop >= convPoint) {
					if ( $qmenuW.hasClass('top')) {
						$qmenuS.css(qmenuScrolledPosition);
						$qmenuW.removeClass('top');
					}
					if ( srlTop >= stopPoint ) {
						if ( !$qmenuW.hasClass('bottom')) {
							$qmenuS.css(qmenuBottomPosition);
							$qmenuW.addClass('bottom');
						}
					} else {
						if ( $qmenuW.hasClass('bottom') ){
							$qmenuS.css(qmenuScrolledPosition);
							$qmenuW.removeClass('bottom');
						}
					}
				} else {
					if ( !$qmenuW.hasClass('top')) {
						if ( $('#shop_topLayer').length > 0 ) {
							$qmenuW.addClass('top');
							$qmenuS.css(qmenuTopPosition);
						}
					}
				}
			}

			//mobile 항시노출
			if( $winW <= 1024 && $qmenuM.is(':visible') && !btn_common_top.hasClass('up')) {
				btn_common_top.addClass('up')
			}
			if ( $('#shop_topLayer').length > 0) {
				$("#footer").css("padding-bottom","135px");
			} else if ( $winW <= 1024 && $qmenuM.is(':visible') ) {
				$("#footer").css("padding-bottom","95px");
			} else {
				$("#footer").css("padding-bottom","35px");
			}
			if ( $winW <= 1024 && viewArea < srlTop ) {
				$btnQ.removeClass('on');
			} else {
				if (!$btnQ.hasClass('on')) {
					$btnQ.addClass('on');
				}
			}
		});

		//mgnb opened
		$('#m_header .m_menu').on('click',function(){
			if ( $('.mm-menu.mm-opened').length > 0 ) {
				$('#qmenu_m').css('bottom','-85px');
			}
		})
		$('.nav_close').on('click',function(){
			$('#qmenu_m').css('bottom','0');
		});
	});

	function callLiveChat() {
		if (!IS_LOGIN) {
			showConfirmBox({
				message : "아티스트 라이브챗을 진행하시려면<br/>로그인이 필요 합니다.<br/><br/>로그인 하시겠습니까?"
				, ok_str : "로그인"
				, cancel_str : "취소하기"
				, ok_func : function () {
					WebBodyStart.goLogin("");
				}
			});
			return;
		}

		var CNTR_CD     = "1007";
		var CNTR_SUB_CD = "";   // 하위코드(예약)
		var CSTMID      = ""; // 통합고객번호  (필수값)
		var CSTMNM      = ""; // 고객명
		var RESIDNO1    = ""; // 생년월일

		if (CSTMID == "") {
			showMessageBox({
				message : "죄송합니다. 아티스트 라이브챗은<br/>뷰티포인트 통합회원만<br/>사용가능합니다."
			});
			return;
		}





		var GEND_DV_CD  = ""; // 성별(남:1, 여:2)


		var ETC         = ""; // 예약

		var parms = '';
		parms += ( 'CNTR_CD'       + '=' + CNTR_CD     );
		parms += ('&CNTR_SUB_CD'   + '=' + CNTR_SUB_CD );
		parms += ('&CSTMID'        + '=' + CSTMID      );
		parms += ('&CSTMNM'        + '=' + CSTMNM      );
		parms += ('&RESIDNO1'      + '=' + RESIDNO1    );
		parms += ('&GEND_DV_CD'    + '=' + GEND_DV_CD  );
		parms += ('&ETC'           + '=' + ETC         );

		webChat(parms);
		
	}
