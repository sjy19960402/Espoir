		, isModalEnd : false
		, success : function (data) {
			if (data.status == "succ") {
				setDataDetailPopupMe(data.object.info);
			}
		}
	});	
}

function setDataDetailPopupMe(data) {
	$("#review_pop_prdnm_me").html(data.v_productnm);
	var rgb = data.v_opt_rgb == undefined ? '' : data.v_opt_rgb;
	rgb = rgb.split(',')[0];

	var html ='';
	if(data.v_optionnm != undefined && data.v_optionnm != "undefined"){
		html = "<p class=\"ccode\"><span style=\"background:#"+rgb+"\"></span>"+data.v_optionnm+"</p>";
		$("#review_pop_color_div_me").html(html);
	}
	
	$("#review_pop_tit_me").html(data.v_title);
	$("#review_pop_cont_me").html(data.v_content);

	var grade = data.n_use_point;
	$("#review_pop_grade_me").removeClass().addClass("ico_grade t0"+grade);
	$("#review_pop_id_me").html(data.v_reg_userid);
	$("#review_pop_date_me").html(data.v_reg_dtm);
	
	html ='';
	if (data.n_photo_cnt > 0) {
		var arrimg = data.v_img_url.split(";");
		var len = arrimg.length; 
		for (var i = 0; i < len; i++ ) {
			html += "<li><img src=\""+arrimg[i]+"\" alt=\"\"></li>";
		}
	}
	
	if ($("#review_pop_slide_me").is(".slick-slider")) 
		$("#review_pop_slide_me").slick("destroy");
	
	$("#review_pop_slide_me").html(html);
	
	if(data.n_photo_cnt > 1) {
		$("#review_pop_slide_me").slick({
			slidesToShow: 1,
			slidesToScroll: 1,
			infinite: true,
			draggable: false,
			dots: true,
			arrows: false,
			autoplay: true
		});
	}
	
	$("#review_pop_slide_me").on('init', function(event, slick, currentSlide, nextSlide){
		$("#review_pop_slide_me").find(".slick-dots > li").eq(0).click();
	});
	
	modalPopup('.modal_review_look_me');
}
</script>
